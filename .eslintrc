{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-trailing-commas",
    "standard-react"
    // "prettier"
  ],
  "plugins": [
    // "prettier"
  ],
  "globals": {
    "graphql": true,
    "__PATH_PREFIX__": true,
    "fetch": true,
    "sessionStorage": true,
    "localStorage": true
  },
  "rules": {
    "react/no-unused-prop-types": "off",
    "react/prop-types": "off",
    "camelcase": "off",
    "react/jsx-handler-names": "off",
    "react/jsx-closing-bracket-location": "off",
    "react/jsx-closing-tag-location": "off"
    // "prettier/prettier": ["error",   {
    // "endOfLine": "lf",
    // "semi": true,
    // "singleQuote": true,
    // "tabWidth": 2,
    // "trailingComma": "es5"
    // }]
  }
}
